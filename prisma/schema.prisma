// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Cache {
  key   String @id
  value Json
  updatedAt DateTime @updatedAt
}

model Counter {
  id    String @id @default("cache")
  count Int
  updatedAt DateTime @updatedAt
}
